<h1> Alurianen News </h1>
<%= link_to 'Add New News Item', new_post_path %>
<div class=".col-md-3">
  </div>
<div class="jumbotron" class=".col-md-6">
<% @posts.each do |post| %>
  <h2><%= post.title %></h2>
  <p><%= post.body %></p>
  <%= link_to 'Edit news item', edit_post_path(post) %>
<%= link_to 'Destroy news item', post, method: :delete %>  
<% end %>
</div>
<div class=".col-md-3">
  </div>

